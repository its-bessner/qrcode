{
    "name": "its-bessner/qrcode",
    "type": "contao-bundle",
    "require": {
        "endroid/qr-code": "*",
        "contao/core-bundle": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "ItsBessner\\Qrcode\\": "src/"
        }
    },
    "authors": [
        {
            "name": "harrie",
            "email": "info@bessner-it.de"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "contao-manager-plugin": "ItsBessner\\Qrcode\\ContaoManager\\Plugin"
    }
}
